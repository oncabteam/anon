{
  "name": "@oncabaret/anon-intent-dashboard",
  "version": "1.0.0",
  "description": "OnCabaret Anonymous Intent Graph Dashboard - Real-time analytics and intent visualization",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "deploy": "npm run build && npm run deploy:s3",
    "deploy:s3": "aws s3 sync dist/ s3://dashboard.oncabaret.com --delete"
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@mapbox/mapbox-gl-geocoder": "^5.0.1",
    "@tanstack/react-query": "^5.8.4",
    "@types/d3": "^7.4.3",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "clsx": "^2.0.0",
    "d3": "^7.8.5",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.14.2",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.15.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.1.6",
    "react-router-dom": "^6.18.0",
    "recharts": "^2.8.0",
    "tailwindcss": "^3.3.5",
    "use-debounce": "^10.0.0",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/lodash": "^4.14.201",
    "@types/mapbox-gl": "^2.7.18",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}