{
  "name": "@oncabaret/anon-intent-backend",
  "version": "1.0.0",
  "description": "OnCabaret Anonymous Intent Graph Backend - Event ingestion and GraphQL API",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "npm run clean && npm run compile",
    "compile": "babel src --out-dir dist --copy-files",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "kafka:start": "docker-compose up -d kafka zookeeper",
    "kafka:stop": "docker-compose down",
    "deploy": "npm run build && npm run deploy:lambda",
    "deploy:lambda": "serverless deploy"
  },
  "keywords": [
    "graphql",
    "apollo-server",
    "event-ingestion",
    "anonymous-analytics",
    "intent-graph",
    "real-time",
    "kafka",
    "dynamodb",
    "lambda"
  ],
  "author": "OnCabaret Team",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.9.0",
    "@apollo/server-express": "^4.9.0",
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/client-kinesis": "^3.450.0",
    "@aws-sdk/client-lambda": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "^3.450.0",
    "apollo-server-lambda": "^3.12.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "mongoose": "^7.6.3",
    "morgan": "^1.10.0",
    "rate-limiter-flexible": "^3.0.8",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@types/jest": "^29.5.7",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "serverless": "^3.36.0",
    "serverless-offline": "^13.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ]
  },
  "babel": {
    "presets": [
      ["@babel/preset-env", {
        "targets": {
          "node": "18"
        }
      }]
    ]
  }
}