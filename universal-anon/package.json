{
  "name": "@oncabaret/anon-intent-sdk",
  "version": "1.0.0",
  "description": "OnCabaret Anonymous Intent Graph SDK - Privacy-first analytics platform for behavioral signals across mobile, web, and digital surfaces",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:sdk && npm run build:backend && npm run build:dashboard",
    "build:sdk": "npm run build:web && npm run build:react-native && npm run build:mobile",
    "build:web": "rollup -c rollup.config.web.js",
    "build:react-native": "rollup -c rollup.config.rn.js", 
    "build:mobile": "npm run build:ios && npm run build:android",
    "build:ios": "cd platforms/ios && swift build",
    "build:android": "cd platforms/android && ./gradlew build",
    "build:backend": "cd backend && npm run build",
    "build:dashboard": "cd dashboard && npm run build",
    "dev": "concurrently \"npm run dev:sdk\" \"npm run dev:backend\" \"npm run dev:dashboard\"",
    "dev:sdk": "rollup -c rollup.config.web.js --watch",
    "dev:backend": "cd backend && npm run dev",
    "dev:dashboard": "cd dashboard && npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "serve:examples": "serve examples/web -p 3000",
    "clean": "rimraf dist && npm run clean:platforms",
    "clean:platforms": "cd backend && npm run clean && cd ../dashboard && npm run clean"
  },
  "keywords": [
    "analytics",
    "intent-tracking", 
    "anonymous",
    "privacy-first",
    "behavioral-signals",
    "react-native",
    "web",
    "nodejs",
    "graphql",
    "real-time",
    "oncabaret",
    "anon-intent-graph"
  ],
  "author": "OnCabaret Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oncabaret/anon-intent-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/oncabaret/anon-intent-sdk/issues"
  },
  "homepage": "https://dashboard.oncabaret.com",
  "workspaces": [
    "sdk",
    "backend", 
    "dashboard",
    "platforms/*",
    "examples/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.41.0",
    "eslint-plugin-jest": "^27.2.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.23.0",
    "serve": "^14.2.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": ">=1.19.0",
    "@react-native-community/netinfo": ">=9.0.0",
    "@react-native-community/geolocation": ">=3.0.0",
    "react": ">=16.8.0",
    "react-native": ">=0.60.0"
  },
  "peerDependenciesMeta": {
    "@react-native-async-storage/async-storage": { "optional": true },
    "@react-native-community/netinfo": { "optional": true },
    "@react-native-community/geolocation": { "optional": true },
    "react": { "optional": true },
    "react-native": { "optional": true }
  },
  "files": [
    "dist",
    "sdk",
    "platforms", 
    "examples",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react-native": {
      "import": "./dist/react-native.esm.js",
      "require": "./dist/react-native.js",
      "types": "./dist/react-native.d.ts"
    },
    "./web": {
      "import": "./dist/web.esm.js",
      "require": "./dist/web.js", 
      "types": "./dist/web.d.ts"
    },
    "./vue": {
      "import": "./dist/vue.esm.js",
      "require": "./dist/vue.js",
      "types": "./dist/vue.d.ts"
    },
    "./angular": {
      "import": "./dist/angular.esm.js",
      "require": "./dist/angular.js",
      "types": "./dist/angular.d.ts"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "testMatch": [
      "**/__tests__/**/*.test.(js|ts|tsx)",
      "**/?(*.)(spec|test).(js|ts|tsx)"
    ],
    "moduleFileExtensions": ["js", "ts", "tsx", "json"],
    "transform": {
      "^.+\\.(js|ts|tsx)$": "babel-jest"
    },
    "collectCoverageFrom": [
      "sdk/**/*.{js,ts,tsx}",
      "platforms/**/*.{js,ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@typescript-eslint/recommended",
      "plugin:jest/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "jest"],
    "rules": {
      "@typescript-eslint/no-explicit-any": "warn",
      "@typescript-eslint/no-unused-vars": "error",
      "no-console": "warn"
    },
    "env": {
      "node": true,
      "browser": true,
      "jest": true
    }
  }
}